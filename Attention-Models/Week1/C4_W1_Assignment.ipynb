                                      \PY{n}{vocab\PYZus{}file}\PY{o}{=}\PY{n}{vocab\PYZus{}file}\PY{p}{,} \PY{n}{vocab\PYZus{}dir}\PY{o}{=}\PY{n}{vocab\PYZus{}dir}\PY{p}{)}\PY{p}{)}
    
