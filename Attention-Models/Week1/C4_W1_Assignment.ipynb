    \PY{n}{ref\PYZus{}counter} \PY{o}{=} \PY{n}{Counter}\PY{p}{(}\PY{n}{reference}\PY{p}{)}
    
    \PY{c+c1}{\PYZsh{} initialize overlap to 0}
